/* playerctl stuff */
ctrl + f8
    playerctl previous

ctrl + f9
    playerctl next

ctrl + f10 
    playerctl play-pause

print 
    ~/dotfiles/scripts/screen.sh

ctrl + delete
    powermenu

ctrl + print
    ~/dotfiles/scripts/figures.sh

ctrl + alt + shift + r 
    ~/dotfiles/scripts/record.sh

ctrl + alt + shift + g
    ~/dotfiles/scripts/gifrecord.sh

ctrl + f5 
    sudo pkill -HUP swhkd
